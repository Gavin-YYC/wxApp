/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background-color: #f0f0f0;
}
.hidden {
  display: none !important;
}
/** 公共flex方法 **/
.flex-row {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
}
.flex-column {
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
}